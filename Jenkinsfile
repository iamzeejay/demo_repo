pipeline {
    agent any

    stages {
        stage('Greet') {
            steps {
                echo 'Hello there!'
            }
        }

        stage('Feature Info') {
            steps {
                echo 'This is the feature1 branch!'
            }
        }

        stage('Thanks') {
            steps {
                echo 'Thank you!'
            }
        }
    }
}
